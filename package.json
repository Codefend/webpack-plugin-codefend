{
  "name": "webpack-plugin-codefend",
  "version": "3.0.2",
  "description": "Webpack plugin for code obfuscation based on Codefend",
  "keywords": [
    "webpack",
    "codefend",
    "webpack-plugin",
    "plugin",
    "obfuscate",
    "obfuscation",
    "obfuscator",
    "uglification",
    "uglify",
    "uglifier",
    "vue",
    "react",
    "angular",
    "svelte"
  ],
  "homepage": "https://github.com/Codefend/webpack-plugin-codefend.git",
  "bugs": {
    "url": "https://github.com/Codefend/webpack-plugin-codefend/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Codefend/webpack-plugin-codefend.git"
  },
  "author": "Mounzer Adas",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "test": "vitest",
    "test:run": "vitest --run",
    "build": "tsup",
    "lint:check": "eslint src --ext .ts",
    "spell:check": "cspell \"**\"",
    "type:check": "tsc --noEmit",
    "prepack": "npm run lint:check && npm run spell:check && npm run type:check && npm run test:run && npm run build"
  },
  "devDependencies": {
    "@types/webpack": "^5.28.5",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "cspell": "^8.10.4",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.3.2",
    "tsup": "^8.1.0",
    "typescript": "^5.5.3",
    "vitest": "^1.6.0",
    "webpack-sources": "^3.2.3"
  },
  "dependencies": {
    "@types/webpack-sources": "^3.2.3",
    "codefend": "^2.1.0",
    "schema-utils": "^4.2.0"
  },
  "peerDependencies": {
    "webpack": "^5.x.x"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
